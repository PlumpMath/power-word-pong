    if (selected) {
        // Leer el teclado.
        if (team == 0 &amp;&amp; keyboard_check(ord('A')) || team == 1 &amp;&amp; keyboard_check(vk_left))
        {
            h_in -= 1;
        }
        if (team == 0 &amp;&amp; keyboard_check(ord('D')) || team==1 &amp;&amp; keyboard_check(vk_right))
        {
            h_in += 1;
        }
        if (team == 0 &amp;&amp; keyboard_check(ord('W')) || team==1 &amp;&amp; keyboard_check(vk_up))
        {
            v_in -=1;
        }
        if (team == 0 &amp;&amp; keyboard_check(ord('S')) || team==1 &amp;&amp; keyboard_check(vk_down))
        {
            v_in +=1;
        }
    }
    
    // Establecer velocidades.
    if (h_in==0)
    {
        h_speed = f_diff(h_speed, acc*3, 0);
    }
    else if (h_in&gt;0)
    {
        h_speed = f_diff(h_speed, acc, max_speed);
    }
    else
    {
        h_speed = f_diff(h_speed, acc, -max_speed);
    }
    
    if (v_in==0)
    {
        v_speed = f_diff(v_speed, acc*3, 0);
    }
    else if (v_in&gt;0)
    {
        v_speed = f_diff(v_speed, acc, max_speed);
    }
    else
    {
        v_speed = f_diff(v_speed, acc, -max_speed);
    }
    
    // Animar.
    sprite_index = sprite_walk;
    if (h_speed==0 &amp;&amp; v_speed==0)
    {
        image_speed = 0;
    }
    else
    {
        image_speed = 3/room_speed;
    }
    
    // Mirar hacia la direcci√≥n correcta.
    if (h_speed!=0) { image_xscale = sign(h_speed); }
    
    // Moverse.
    x += h_speed;
    y += v_speed;
}
else if (is_attacking)
{

    sprite_index = sprite_attack;
    image_speed = 5/room_speed;
}
</string>
